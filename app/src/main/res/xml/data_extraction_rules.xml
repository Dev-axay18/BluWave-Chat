<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="crypto_keys.xml"/>
        <exclude domain="sharedpref" path="bluetooth_config.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="sharedpref" path="crypto_keys.xml"/>
        <exclude domain="sharedpref" path="bluetooth_config.xml"/>
    </device-transfer>
</data-extraction-rules>
